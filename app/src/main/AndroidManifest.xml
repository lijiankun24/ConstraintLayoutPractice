<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.lijiankun24.constraintlayoutpractice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/BaseAppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".RelativePositionActivity">
        </activity>
        <activity android:name=".MarginsActivity">
        </activity>
        <activity android:name=".CenteringPositionActivity">
        </activity>
        <activity android:name=".CircularPositionActivity">
        </activity>
        <activity android:name=".VisibilityBehaviorActivity">
        </activity>
        <activity android:name=".DimensionConstraintsActivity">
        </activity>
        <activity android:name=".ChainsActivity">
        </activity>
        <activity android:name=".VirtualHelpersActivity">
        </activity>
    </application>

</manifest>